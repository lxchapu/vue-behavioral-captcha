{
  "name": "vue-behavioral-captcha",
  "version": "0.1.2",
  "description": "基于 Vue 3 的行为式验证码",
  "repository": {
    "type": "git",
    "url": "https://github.com/lxchapu/vue-behavioral-captcha.git"
  },
  "keywords": [
    "vue-behavioral-captcha",
    "behavioral-captcha",
    "puzzle",
    "vue3"
  ],
  "license": "MIT",
  "packageManager": "^pnpm@7.26.3",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-behavioral-captcha.umd.cjs",
  "module": "./dist/vue-behavioral-captcha.js",
  "exports": {
    ".": {
      "import": "./dist/vue-behavioral-captcha.js",
      "require": "./dist/vue-behavioral-captcha.umd.cjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
